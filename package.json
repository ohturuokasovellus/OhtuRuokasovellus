{
  "name": "ruokasovellus",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:server": "node backend/index.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export:web",
    "lint": "eslint ./{src,backend,e2e-test}/**/*.{js,jsx} ./{src,backend,e2e-test}/*.{js,jsx} App.jsx --no-error-on-unmatched-pattern",
    "test": "jest --coverage",
    "test:e2e": "E2ETEST=1 playwright test"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/e2e-tests/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.0.2",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "expo": "^51.0.2",
    "expo-constants": "~16.0.1",
    "expo-font": "~12.0.4",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.11",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "express": "^4.19.2",
    "formik": "^2.4.6",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "react-native-web": "~0.19.10",
    "react-qr-code": "^2.0.13",
    "react-router-native": "^6.23.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.24.5",
    "@expo/webpack-config": "^19.0.1",
    "@playwright/test": "^1.44.0",
    "@stylistic/eslint-plugin-js": "^2.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.1",
    "react-router-dom": "^6.23.1",
    "react-test-renderer": "18.2.0",
    "supertest": "^7.0.0",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "@expo/webpack-config": {
      "expo": "^51"
    }
  },
  "private": true
}
